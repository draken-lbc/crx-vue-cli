{
  "name": "crx-vue-cli",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@types/chrome": "0.0.220",
    "@types/node": "^18.14.6",
    "@vitejs/plugin-vue": "^4.0.0",
    "less": "^4.1.3",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vue-tsc": "^1.0.24"
  },
  "manifest3": {
    "name": "crx-vue-cli",
    "description": "爬取钉钉文档数据",
    "manifest_version": 3,
    "host_permissions": [
      "<all_urls>"
    ],
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "index.js"
        ],
        "run_at": "document_end"
      }
    ],
    "permissions": [
      "contextMenus",
      "notifications",
      "cookies",
      "tabs",
      "webRequest",
      "power",
      "webRequestBlocking"
    ]
  }
}
